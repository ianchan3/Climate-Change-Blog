<%- include('../partials/header') %>

<link rel="stylesheet" href="/stylesheets/indexpage.css">

<table id="list">
  <thead>
    <tr>
      <th>Articles</th>
      <th>Published<br>Date</th>
      <th>Details</th>
      <td><a class="button" href="/articles/new">New Article</a></td>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% articles.forEach(function(a) { %>
      <tr>
        <td><%= a.title %></td>
        <td><%= a.publishedDate %></td>
        <td><a class="button" href="/articles/<%= a._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>