<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Articles</th>
      <th>Release<br>Date</th>
      <th>Details</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% articles.forEach(function(a) { %>
      <tr>
        <td><%= a.Articles %></td>
        <td><%= a.releaseDate %></td>
        <td><a href="/articles/<%= a._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
<%- include('../partials/footer') %>